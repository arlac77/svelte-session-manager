{
  "name": "svelte-session-manager",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/index.mjs",
  "svelte": "./src/index.svelte",
  "description": "Session store for svelte (currently only for JWT)",
  "keywords": [
    "JWT",
    "component",
    "session",
    "svelte",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "rollup -c example/rollup.config.mjs -w",
    "test": "testcafe $BROWSER:headless tests/*-test.js -s test-results --app-init-delay 1000 --app \"rollup -c example/rollup.config.mjs -w\"",
    "docs": "documentation readme src/index.mjs --section=API",
    "lint": "documentation lint src/index.mjs"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.1.0",
    "documentation": "^13.0.1",
    "jsonwebtoken": "^8.5.1",
    "rollup": "^2.18.0",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-svelte": "git+https://github.com/ctavan/rollup-plugin-svelte.git#handle-missing-exports-gracefully",
    "semantic-release": "^17.1.0",
    "svelte": "^3.23.2",
    "testcafe": "^1.8.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-session-manager.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-session-manager/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-session-manager#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-svelte-component"
    ]
  }
}
