{
  "name": "svelte-session-manager",
  "version": "0.0.0-semantic-release",
  "module": "src/index.mjs",
  "svelte": "src/index.svelte",
  "description": "Session store for svelte (currently only for JWT)",
  "keywords": [
    "svelte",
    "session",
    "JWT"
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "rollup -c example/rollup.config.js -w",
    "test": "testcafe $BROWSER:headless tests/*-test.js -s test-results --app-init-delay 500 --app \"rollup -c example/rollup.config.js -w\"",
    "docs": "documentation readme src/index.mjs --section=API",
    "lint": "documentation lint src/index.mjs"
  },
  "devDependencies": {
    "documentation": "^12.1.4",
    "jsonwebtoken": "^8.5.1",
    "rollup": "^2.0.3",
    "rollup-plugin-dev": "^1.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.1",
    "semantic-release": "^17.0.4",
    "svelte": "^3.19.2",
    "testcafe": "^1.8.2"
  },
  "engines": {
    "node": ">=12.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-session-manager.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-session-manager/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-session-manager#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-svelte",
      "arlac77/template-github"
    ]
  }
}
